{
    "name": "nabilanam/simpleupload",
    "description": "A dead simple file uploader",
    "license": "MIT",
    "authors": [
        {
            "name": "Nabil Anam",
            "email": "nabil_code@outlook.com",
            "homepage": "https://github.com/nabilanam"
        }
    ],
    "homepage": "https://github.com/nabilanam/simpleupload",
    "keywords": [
        "Laravel",
        "Simple",
        "File",
        "Upload"
    ],
    "require": {
        "illuminate/support": "~5|~6",
        "intervention/image": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "NabilAnam\\SimpleUpload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NabilAnam\\SimpleUpload\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NabilAnam\\SimpleUpload\\SimpleUploadServiceProvider"
            ],
            "aliases": {
                "SimpleUpload": "NabilAnam\\SimpleUpload\\Facades\\SimpleUpload"
            }
        }
    }
}